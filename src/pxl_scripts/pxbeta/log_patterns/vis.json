{
    "variables": [
        {
            "name": "start_time",
            "type": "PX_STRING",
            "description": "The start time of the window in time units before now.",
            "defaultValue": "-5m"
        },
        {
            "name": "source_filter",
            "type": "PX_STRING",
            "description": "The partial string to match the 'source' column.",
            "defaultValue": ""
        },
        {
            "name": "destination_filter",
            "type": "PX_STRING",
            "description": "The partial string to match the 'destination' column.",
            "defaultValue": ""
        },
        {
            "name": "max_num_records",
            "type": "PX_INT64",
            "description": "Max number of records to show.",
            "defaultValue": "1000"
        }
    ],
    "globalFuncs": [
        {
            "outputName": "log_pattern_data",
            "func": {
                "name": "log_pattern_data",
                "args": [
                    {
                        "name": "start_time",
                        "variable": "start_time"
                    },
                    {
                        "name": "source_filter",
                        "variable": "source_filter"
                    },
                    {
                        "name": "destination_filter",
                        "variable": "destination_filter"
                    },
                    {
                        "name": "num_head",
                        "variable": "max_num_records"
                    }
                ]
            }
        },
        {
            "outputName": "log_pattern_grouped_by",
            "func": {
                "name": "log_pattern_grouped_by",
                "args": [
                    {
                        "name": "start_time",
                        "variable": "start_time"
                    },
                    {
                        "name": "source_filter",
                        "variable": "source_filter"
                    },
                    {
                        "name": "destination_filter",
                        "variable": "destination_filter"
                    },
                    {
                        "name": "num_head",
                        "variable": "max_num_records"
                    }
                ]
            }
        }
    ],
    "widgets": [
        {
            "name": "Time series",
            "globalFuncOutputName": "log_pattern_data",
            "displaySpec": {
                "@type": "types.px.dev/px.vispb.TimeseriesChart",
                "timeseries": [{
                    "value": "count",
                    "series": "container_id",
                    "mode": "MODE_AREA",
                    "stackBySeries": "level"
                }],
                "title": "",
                "yAxis": {
                    "label": "Log pattern counts"
                },
                "xAxis": null
            },
            "position": {
                "x": 0,
                "y": 0,
                "w": 12,
                "h": 4
            }
        },
        {
            "name": "Table",
            "position": {
                "x": 0,
                "y": 3,
                "w": 12,
                "h": 4
            },
            "globalFuncOutputName": "log_pattern_grouped_by",
            "displaySpec": {
                "@type": "types.px.dev/px.vispb.Table"
            }
        }
    ]
}
