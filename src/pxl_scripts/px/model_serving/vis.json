{
    "variables": [
        {
            "name": "svc",
            "type": "PX_SERVICE",
            "description": "The full/partial name of the service to get stats for. Format: ns/svc_name",
            "defaultValue": ""
        },
        {
            "name": "start_time",
            "type": "PX_STRING",
            "description": "The relative start time of the window. Current time is assumed to be now",
            "defaultValue": "-5m"
        },
        {
            "name": "exclude_in_cluster",
            "type": "PX_BOOLEAN",
            "description": "Whether in-cluster LLM APIs should be excluded from the response",
            "defaultValue": false
        },
        {
            "name": "exclude_external",
            "type": "PX_BOOLEAN",
            "description": "Whether external LLM APIs should be excluded from the response",
            "defaultValue": false
        }
    ],
    "globalFuncs": [
        {
            "outputName": "LET",
            "func": {
                "name": "svc_let",
                "args": [
                    {
                        "name": "start_time",
                        "variable": "start_time"
                    },
                    {
                        "name": "svc",
                        "variable": "svc"
                    },
                    {
                        "name": "exclude_in_cluster",
                        "variable": "exclude_in_cluster"
                    },
                    {
                        "name": "exclude_external",
                        "variable": "exclude_external"
                    }
                ]
            }
        }
    ],
    "widgets": [
        {
            "name": "Time to first token p50 Latency",
            "position": {
                "x": 0,
                "y": 0,
                "w": 4,
                "h": 3
            },
            "globalFuncOutputName": "LET",
            "displaySpec": {
                "@type": "types.px.dev/px.vispb.TimeseriesChart",
                "timeseries": [
                    {
                        "value": "ttft_latency_p50",
                        "series": "k8s",
