{
    "variables": [
        {
            "name": "start_time",
            "type": "PX_STRING",
            "description": "The start time of the window in time units before now.",
            "defaultValue": "-5m"
        },
        {
            "name": "mcp_session_id",
            "type": "PX_STRING",
            "description": "The Mcp-Session-Id to filter on. If empty, no filtering is done.",
            "defaultValue": ""
        },
        {
            "name": "source_filter",
            "type": "PX_STRING",
            "description": "The partial string to match the 'source' column.",
            "defaultValue": ""
        },
        {
            "name": "destination_filter",
            "type": "PX_STRING",
            "description": "The partial string to match the 'destination' column.",
            "defaultValue": ""
        },
        {
            "name": "max_num_records",
            "type": "PX_INT64",
            "description": "Max number of records to show.",
            "defaultValue": "1000"
        }
    ],
    "globalFuncs": [
        {
            "outputName": "mcp_data",
            "func": {
                "name": "mcp_data",
                "args": [
                    {
                        "name": "start_time",
                        "variable": "start_time"
                    },
                    {
                        "name": "mcp_session_id",
                        "variable": "mcp_session_id"
                    },
                    {
                        "name": "source_filter",
                        "variable": "source_filter"
                    },
                    {
                        "name": "destination_filter",
                        "variable": "destination_filter"
                    },
                    {
                        "name": "num_head",
                        "variable": "max_num_records"
                    }
                ]
            }
        }
    ],
    "widgets": [
        {
            "name": "Table",
            "position": {
                "x": 0,
                "y": 0,
                "w": 12,
                "h": 4
            },
            "globalFuncOutputName": "mcp_data",
            "displaySpec": {
                "@type": "types.px.dev/px.vispb.Table"
            }
        }
    ]
}
